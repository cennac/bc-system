<form class="bc-page" data-type='form' style="overflow: auto;"
	data-namespace="bswf.generalOrder.HumanResourceDepartmentCooperationForm"
	data-js='js:bc_identity,bc-business/bs.js,wf:HumanResourceDepartmentCooperation2JS_${pd_id}.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
		<input type="hidden" name="list_hrdc_applyMatters" data-scope="global" data-type="list"/>
		<input type="hidden" name="list_hrdc_applyMatters2local" data-type="list"/>
		<table class="ui-widget-content bc-grid" id="applyMatters" cellspacing="0" cellpadding="0" style="margin-bottom:4px;width:100%;border-width:0 0 0 0;">
			<tr class="header row">
				<td class="first" style="width: 2.5em;padding:0;">序号</td>
				<td class="middle" style="width: 30em;padding:0;">申请事项</td>
				<td class="middle" style="width: 15em;padding:0;">协办意见</td>
				<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
			</tr>
			<#if list_am_applyMatters??>
			<#list list_am_applyMatters as am>
			<tr class="ui-widget-content row">
				<td class="first" style="vertical-align:top;">${am.id!}</td>
				<td class="middle" style="margin:0;vertical-align:top;padding:0 2 0 2px;">
					<div  style="text-align:left;vertical-align:top;word-break:break-all;white-space:normal;">${am.matter!}</div>
				</td>
				<td class="middle" style="margin:0;padding:0;">
					<div class="ignore editable input"  style="width:100%;height:100%;border:none;background:none;padding:0 0 0 2px; overflow-x:hidden;overflow-y:auto;word-break:break-all;white-space:normal;"  data-validate="required" ></div>
				</td>
				<td class="last" style="border-width:1px 1px 0 1px;"></td>
			</tr>
			</#list>
			</#if>
		</table>
	<#else>	
		<table class="ui-widget-content bc-grid"  cellspacing="0" cellpadding="0" style="margin-bottom:4px;width:100%;border-width:0 0 0 0;">
			<tr class="header row">
				<td class="first" style="width: 2.5em;padding:0;">序号</td>
				<td class="middle" style="width: 30em;padding:0;">申请事项</td>
				<td class="middle" style="width: 15em;padding:0;">协办意见</td>
				<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
			</tr>
			<#if list_hrdc_applyMatters2local??>
				<#list list_hrdc_applyMatters2local as am>
					<tr class="ui-widget-content row">
						<td class="first" style="vertical-align:top;">${am.id!}</td>
						<td class="middle" style="margin:0;padding:2 0 0 0px;">
							<div  style="text-align:left;vertical-align:top;word-break:break-all;white-space:normal;">${am.matter!}</div>
						</td>
						<td class="middle" style="margin:0;padding:2 0 0 0px;">
							<div  style="text-align:left;vertical-align:top;word-break:break-all;white-space:normal;">${am.view!}</div>
						</td>
						<td class="last" style="border-width:1px 1px 0 1px;"></td>
					</tr>
				</#list>
			<#else>
				<#list list_am_applyMatters as am>
					<tr class="ui-widget-content row">
						<td class="first" style="vertical-align:top;">${am.id!}</td>
						<td class="middle" style="margin:0;vertical-align:top;padding:0 2 0 2px;">
							<div  style="text-align:left;vertical-align:top;word-break:break-all;white-space:normal;">${am.matter!}</div>
						</td>
						<td class="middle">
						</td>
						<td class="last" style="border-width:1px 1px 0 1px;"></td>
					</tr>
				</#list>
			</#if>
		</table>
	</#if>
</form>