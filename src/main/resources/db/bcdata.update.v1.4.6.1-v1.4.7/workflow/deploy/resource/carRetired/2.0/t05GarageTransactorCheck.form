				<td class="middle" >${gtc_device_DDZZDesc!}</td>
				<td class="last" style="border-width:0 0 0 1px;"></td>
			</tr>
			<tr class="row">
				<td class="first" >计价器</td>
				<td class="middle" >${gtc_device_JJQ!}</td>
				<td class="middle" >${gtc_device_JJQDesc!}</td>
				<td class="last" style="border-width:0 0 0 1px;"></td>
			</tr>
			<tr class="row">
				<td class="first" >远距离密标</td>
				<td class="middle" >${gtc_device_YJLMB!}</td>
				<td class="middle" >${gtc_device_YJLMBDesc!}</td>
				<td class="last" style="border-width:0 0 0 1px;"></td>
			</tr>
			<tr class="row">
				<td class="first" >近距离密标</td>
				<td class="middle" >${gtc_device_JJLMB!}</td>
				<td class="middle" >${gtc_device_JJLMBDesc!}</td>
				<td class="last" style="border-width:0 0 0 1px;"></td>
			</tr>
			<tr class="row">
