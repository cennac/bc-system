<form class="bc-page" data-type='form' style="overflow: auto;"
	data-namespace="bswf.carRetired.GarageTransactorCheckForm"
	data-js='wf:GarageTransactorCheck2JS_${pd_id}.js'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<#if readonly == 'false'>
