<form class="bc-page" data-type='form' style="overflow-y:auto;overflow-x:hidden;"
	data-namespace="bswf.carManEntry.recruiterInformDriverRetestForm"
	data-js='<#if readonly == "false">js:bc_identity,bc-business/bs.js,wf:t060RecruiterInformDriverRetest2JS_${pd_id}.js</#if>'
	data-readonly="<#if readonly??>${readonly}<#else>false</#if>">
	<div style="width:640px;">
	<#if readonly == 'false'>
		<input type="hidden" name="subject" data-scope="global"  value="${subject}" />
		<input type="hidden" name="name" class="ignore"  value="${name}" />
		<input type="hidden" name="applyAttr" class="ignore"  value="${applyAttr}" />
		<div style="padding-left:0.5em;">
			*司机参加复试：<input type="hidden" name="isGiveUp_lc" data-type="boolean"/>
			<input type="hidden" name="isGiveUp" data-scope="global" data-type="boolean" />
			<input type="radio"  name="risGiveUp" value="false" class="ignore" 	style="width:auto;" />
			<label>同意</label>
			<input type="radio"  name="risGiveUp" value="true"  class="ignore" 	style="width:auto;" />
			<label>放弃</label>
		</div>
		<div id="testConfig" style="display:none;">
			
			<input type="hidden" name="writtenTestFlow" 	data-scope="global"	data-type="boolean" value="false" class="ignore"/>
			<input type="hidden" name="interviewFlow1" 		data-scope="global"	data-type="boolean" value="false" class="ignore"/>
			<input type="hidden" name="interviewFlow2" 		data-scope="global"	data-type="boolean" value="false" class="ignore"/>
			<input type="hidden" name="interviewFlow3" 		data-scope="global"	data-type="boolean" value="false" class="ignore"/>
			<input type="hidden" name="interviewFlow4" 		data-scope="global"	data-type="boolean" value="false" class="ignore"/>
			<input type="hidden" name="interviewFlow5" 		data-scope="global"	data-type="boolean" value="false" class="ignore"/>
			<input type="hidden" name="roadTestFlow" 		data-scope="global"	data-type="boolean" value="false" class="ignore"/>
			
			<input type="hidden" name="writtenTestExaminer" 	data-scope="global"	class="ignore"/>
			<input type="hidden" name="interviewExaminer1" 		data-scope="global"	class="ignore"/>
			<input type="hidden" name="interviewExaminer2" 		data-scope="global"	class="ignore"/>
			<input type="hidden" name="interviewExaminer3" 		data-scope="global"	class="ignore"/>
			<input type="hidden" name="interviewExaminer4" 		data-scope="global"	class="ignore"/>
			<input type="hidden" name="interviewExaminer5" 		data-scope="global"	class="ignore"/>
			<input type="hidden" name="roadTestExaminer" 		data-scope="global"	class="ignore"/>
		
			<input type="hidden" name="list_writtenCheck4assignee" 		data-scope="global"	data-type="list" 	class="ignore"/>
			<input type="hidden" name="list_interviewDriver4assignee" 	data-scope="global" data-type="list" 	class="ignore"/>
			<input type="hidden" name="list_drivingCheck4assignee" 		data-scope="global" data-type="list" 	class="ignore"/>
			
			<div style="border-width: 0;padding-left: 0.5em;">
				<span class="text">*复试配置：(申请属性为<b><#if applyAttr??><#if applyAttr=="NewEntry">外司加入<#elseif applyAttr=="NewTest">新考<#elseif applyAttr=="KeepOn">留用</#if></#if></b>)</span>
			</div>
			<table class="ui-widget-content bc-grid" cellspacing="0" cellpadding="0" style="margin-bottom:4px;width:100%;border-width:0 0 0 0;">
				<tr class="header row">
					<td class="first" 	style="width: 6em;border-top:1px solid #CCC;">复试科目</td>
					<td class="middle" 	style="width: 30em;border-top:1px solid #CCC;">考官人员配置</td>
					<td class="last" 	style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
				<tr class="row">
					<td class="first"  style="padding:0;text-align:center;">笔试</td>
					<td class="middle" style="padding:0;text-align:left;">	
						<div style="position:relative;margin: 0;padding: 1px 0;min-height:19px;margin: 0;font-weight: normal;width: 98%;" >
							<span class="selectTransactor selectButton verticalMiddle ui-icon ui-icon-circle-plus"  title="点击添加笔试考官"></span>
							<ul class="horizontal" style="padding: 0;overflow:hidden;">
								<#if applyAttr!="KeepOn">
								<li class="horizontal  ui-widget-content ui-corner-all ui-state-highlight" data-id="10116814" data-hidden="{'id':'10116814','code':'xu','name':'陈旭明'}" style="position: relative;margin:0 2px;float: left;padding: 0;border-width: 0;"><span class="text">陈旭明</span><span class="click2remove verticalMiddle ui-icon ui-icon-close" style="margin: -8px -2px;" title="点击移除"></span></li>
								</#if>
							</ul>
						</div>
						<input type="hidden" value="CarManEntryWrittenTestExaminer" class="ignore bswf-carManEntry-ignore"/>
					</td>
					<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
				<tr class="row">
					<td class="first"  style="padding:0;text-align:center;">面试</td>
					<td class="middle" style="padding:0;text-align:left;">	
						<div style="position:relative;margin: 0;padding: 1px 0;min-height:19px;margin: 0;font-weight: normal;width: 98%;" >
							<span class="selectTransactor selectButton verticalMiddle ui-icon ui-icon-circle-plus"  title="点击添加面试考官"></span>
							<ul class="horizontal" style="padding: 0;overflow:hidden;"></ul>
						</div>
						<input type="hidden" value="CarManEntryInterviewExaminer" class="ignore bswf-carManEntry-ignore"/>
					</td>
					<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
				<tr class="row">
					<td class="first"  style="padding:0;text-align:center;">路试</td>
					<td class="middle" style="padding:0;text-align:left;">	
						<div style="position:relative;margin: 0;padding: 1px 0;min-height:19px;margin: 0;font-weight: normal;width: 98%;" >
							<span class="selectTransactor selectButton verticalMiddle ui-icon ui-icon-circle-plus"  title="点击添加路试考官"></span>
							<ul class="horizontal" style="padding: 0;overflow:hidden;">
								<#if applyAttr!="KeepOn">
								<li class="horizontal  ui-widget-content ui-corner-all ui-state-highlight" data-id="10125320" data-hidden="{'id':'10125320','code':'ming','name':'陈纪铭'}" style="position: relative;margin:0 2px;float: left;padding: 0;border-width: 0;"><span class="text">陈纪铭</span><span class="click2remove verticalMiddle ui-icon ui-icon-close" style="margin: -8px -2px;" title="点击移除"></span></li>
								<li class="horizontal  ui-widget-content ui-corner-all ui-state-highlight" data-id="10125220" data-hidden="{'id':'10125220','code':'yangjianxin','name':'杨建新'}" style="position: relative;margin:0 2px;float: left;padding: 0;border-width: 0;"><span class="text">杨建新</span><span class="click2remove verticalMiddle ui-icon ui-icon-close" style="margin: -8px -2px;" title="点击移除"></span></li>
								</#if>
								</ul>
						</div>
						<input type="hidden" value="CarManEntryRoadTestExaminer" class="ignore bswf-carManEntry-ignore"/>
					</td>
					<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
			</table>
		</div>
		<div id="RetestHeadCheck" style="padding-left:0.5em;display:none;" class="bc-relativeContainer" >
			<div class="bc-relativeContainer">*复试组长：</div>
			<div class="row bc-relativeContainer" style="width:10em;position:inline-block;">
				<input type="text" 		name="RetestHead4Name" class="ignore ui-widget-content" value="陈金亮" readonly="readonly" data-scope="global" />
				<input type="hidden"	name="RetestHead"      class="ignore" data-scope="global" value="cjl" />
				<input type="hidden"	name="RetestHead4Id"   class="ignore" data-scope="global" value="100698" data-type="long" />
				<ul class="inputIcons">
					<li id="selectRetestHead"     class="inputIcon ui-icon ui-icon-circle-plus" title='点击添加复试组长'></li>
				</ul>
			</div>
		</div>
		<div id="RetestArrange" style="padding-left:0.5em;display:none;" class="bc-relativeContainer" >
			<div class="bc-relativeContainer">*复试日期：</div>
			<div class="bc-relativeContainer" style="width:10em;position:inline-block;">
				<div class="relative">
					<input type="text" name="interviewDate" class="bc-date ui-widget-content ignore"  data-scope="global" value="${interviewDate!}" />
					<ul class="inputIcons">
						<li class="selectCalendar inputIcon ui-icon ui-icon-calendar"></li>
					</ul>
				</div>
			</div>
		</div>
		<div id="giveUp4cause" style="padding-left:0.5em;display:none;" >
			<div style="display:inline-block;vertical-align:top;">*放弃原因：</div>
			<textarea name="giveUp4cause"  data-scope="global" rows="3" style="width:80%;" class="ignore ui-widget-content noresize" ></textarea>
			<input type="hidden" name="giveUp4cause_lc" class="ignore" />
		</div>
	<#else>
		<div style="padding-left:0.5em;">
			>司机参加复试：<#if isGiveUp_lc??><#if isGiveUp_lc>放弃<#else>同意</#if></#if>
		</div>
		<#if isGiveUp_lc??&&!isGiveUp_lc>
			<div style="border-width: 0;padding-left: 0.5em;">
				<span class="text">>复试配置：</span>
			</div>
			<table class="ui-widget-content bc-grid" cellspacing="0" cellpadding="0" style="margin-bottom:4px;width:100%;border-width:0 0 0 0;">
				<tr class="header row">
					<td class="first" 	style="width: 10em;border-top:1px solid #CCC;">复试科目</td>
					<td class="middle" 	style="width: 30em;border-top:1px solid #CCC;">考官人员配置</td>
					<td class="last" 	style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
				<tr class="row">
					<td class="first"  style="padding:0;text-align:center;">笔试</td>
					<td class="middle" style="padding:0;text-align:left;">	
						<#list list_writtenCheck4assignee as a>
							${a.assigneeName!}<#if a_has_next>,</#if>
						</#list>
					</td>
					<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
				<tr class="row">
					<td class="first"  style="padding:0;text-align:center;">面试</td>
					<td class="middle" style="padding:0;text-align:left;">	
						<#list list_interviewDriver4assignee as a>
							${a.assigneeName!}<#if a_has_next>,</#if>
						</#list>
					</td>
					<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
				<tr class="row">
					<td class="first"  style="padding:0;text-align:center;">路试</td>
					<td class="middle" style="padding:0;text-align:left;">	
						<#list list_drivingCheck4assignee as a>
							${a.assigneeName!}<#if a_has_next>,</#if>
						</#list>
					</td>
					<td class="last" style="min-width: 0.01em;border-width:1px 1px 0 1px;"></td>
				</tr>
			</table>

			<div style="border-width: 0;padding-left: 0.5em;">
				>复试组长：${RetestHead4Name!}
			</div>
			<div style="border-width: 0;padding-left: 0.5em;">
				>复试日期：<b>${interviewDate!}</b>
			</div>
		</#if>
		<#if giveUp4cause_lc??&&giveUp4cause_lc!="">
			<div style="padding-left:0.5em;">
				<div style="display:inline-block;vertical-align:top;">放弃原因：</div>
				<div class="text" style="word-break: break-all;display:inline-block;width:80%;">${giveUp4cause_lc!}</div>
			</div>
		</#if>
	</#if>
	</div>
</form>